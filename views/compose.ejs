<%- include('partials/header') %>

    <h1>Compose</h1>


    <form class="" action="/compose" method="post">
        <div class="form-group">
            <div class="mb-3">
                <label for="title" class="form-label">Title</label>
                <input type="text" class="form-control" id="" name="postTitle">
            </div>
            <div class="mb-3">
                <label for="content" class="form-label">Post</label>
                <textarea name="postBody" id="" cols="30" rows="10" class="form-control"></textarea>
            </div>
            
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>



    </form>














